<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.CreateBoardCtrl">
   <children>
      <TextField fx:id="fieldBoardName" layoutX="89.0" layoutY="169.0" prefHeight="26.0" prefWidth="302.0" promptText="Enter a title." onAction="#clickCreateBoard"/>
      <Label layoutX="153.0" layoutY="83.0" text="Create a New Board">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button fx:id="buttonCreate" layoutX="419.0" layoutY="169.0" mnemonicParsing="false" onAction="#clickCreateBoard" prefHeight="26.0" prefWidth="90.0" text="Create" />
      <Button fx:id="buttonBack" layoutX="27.0" layoutY="32.0" mnemonicParsing="false" onAction="#clickBackHome">
         <graphic>
            <ImageView fx:id="myImage" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <Image url="https://cdn-icons-png.flaticon.com/512/0/340.png"/>
            </ImageView>
         </graphic>
         <tooltip><Tooltip text="Return to Homepage" /></tooltip>
      </Button>
   </children>
</AnchorPane>
